############
#
# Example submit file for vanilla job
#
############

getenv = True
request_cpus = 2

+request_gpus = 1

Requirements            = TARGET.vm_name == "ITS-U18-NFS-20191029_gpu"


Universe       = vanilla
Executable     = /home/jkotary/fultr/run_hyperparams.sh
Arguments      = $(a) $(b) $(c) $(d) $(e) $(f) $(g) $(h)


input   = /dev/null
output  = /home/jkotary/fultr/out/run_$(h).out
error   = /home/jkotary/fultr/err/run_$(h).err

Queue a,b,c,d,e,f,g,h from run_hyperparams.in
